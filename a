				optionButton.Text = ""
				optionButton.BackgroundColor3 = Color3.fromRGB(30.00000011175871, 30.00000011175871, 30.00000011175871)
				optionButton.BackgroundTransparency = 0.5
				optionButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
				optionButton.BorderSizePixel = 0
				optionButton.LayoutOrder = 1
				optionButton.Position = UDim2.new(-0.00150008628, 0, 0, 0)
				optionButton.Size = UDim2.new(0.925999999, 0, 0.25, 0)
				optionButton.Name = "Option"
				optionButton.Parent = mainDropdown

				optionStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
				optionStroke.Color = Color3.fromRGB(122.00000792741776, 97.00000941753387, 223.00001710653305)
				optionStroke.Thickness = 1.5
				optionStroke.Parent = optionButton

				optionGradient.Color = ColorSequence.new{
					ColorSequenceKeypoint.new(0, Color3.fromRGB(45.00000111758709, 74.0000031888485, 133.00000727176666)),
					ColorSequenceKeypoint.new(1, Color3.fromRGB(162.00000554323196, 81.00000277161598, 206.0000029206276))
				}
				optionGradient.Rotation = 73
				optionGradient.Parent = optionButton

				optionCorner.CornerRadius = UDim.new(0.100000001, 0)
				optionCorner.Parent = optionButton
				
				optionTextSizeConstraint.MaxTextSize = 25
				optionTextSizeConstraint.Parent = optionButton	

				optionTitle.Font = Enum.Font.GothamBold
				optionTitle.Text = option
				optionTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
				optionTitle.TextScaled = true
				optionTitle.TextSize = 14
				optionTitle.TextWrapped = true
				optionTitle.TextXAlignment = Enum.TextXAlignment.Left
				optionTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				optionTitle.BackgroundTransparency = 1
				optionTitle.Position = UDim2.new(0.035458345, 0, 0.18378675, 0)
				optionTitle.Size = UDim2.new(0.820081115, 0, 0.605058491, 0)
				optionTitle.Name = "Title"
				optionTitle.Parent = optionButton

				optionTitleTextSizeConstraint.MaxTextSize = 30
				optionTitleTextSizeConstraint.Parent = optionTitle
				
				optionButton.MouseButton1Click:Connect(function()
					dropdownToggle = false
					
					dropdownScript(option)
					
					selectionTitle.Text = optionTitle.Text

					newDropdown:TweenSize(UDim2.new(0.927, 0,0, 0), "In", "Sine", 1)
					task.wait(1)
					newDropdown.Visible = false
				end)
			end
		end
		
		return elements
	end
	
	return hub
end

return Library
