			toggleTitle.TextSize = 14
			toggleTitle.TextWrapped = true
			toggleTitle.TextXAlignment = Enum.TextXAlignment.Left
			toggleTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			toggleTitle.BackgroundTransparency = 1
			toggleTitle.Position = UDim2.new(0.035458345, 0, 0.18378675, 0)
			toggleTitle.Size = UDim2.new(0.820081115, 0, 0.605058491, 0)
			toggleTitle.Name = "Title"
			toggleTitle.Parent = toggleFrame

			toggleTitleTextConstraint.MaxTextSize = 30
			toggleTitleTextConstraint.Parent = toggleTitle
			
			toggleButton.MouseButton1Click:Connect(function()
				if not status then
					tweenService:Create(toggleButtonInner, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(122, 97, 223)}):Play()
					tweenService:Create(toggleButtonInner, TweenInfo.new(0.5), {Position = UDim2.new(0.5, 0,0.091, 0)}):Play()
					task.wait(0.5)
					status = true
				else
					tweenService:Create(toggleButtonInner, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()
					tweenService:Create(toggleButtonInner, TweenInfo.new(0.5), {Position = UDim2.new(0.101, 0,0.091, 0)}):Play()
					task.wait(0.5)
					status = false
				end
				
				pcall(toggleScript, status)
			end)
		end
		
		function elements.createDropdown(title, list, dropdownScript)
