			mainDropdownListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
			mainDropdownListLayout.SortOrder = Enum.SortOrder.LayoutOrder
			mainDropdownListLayout.Parent = mainDropdown
			
			for _, option in pairs(list) do
				local optionButton = Instance.new("TextButton")
				local optionStroke = Instance.new("UIStroke")
				local optionGradient = Instance.new("UIGradient")
				local optionCorner = Instance.new("UICorner")
