			inputTitle.Size = UDim2.new(0.707024574, 0, 0.605058491, 0)
			inputTitle.Name = "Title"
			inputTitle.Parent = input

			inputTitleTextConstraint.MaxTextSize = 30
			inputTitleTextConstraint.Parent = inputTitle
			
			mainInput.FocusLost:Connect(function(enterPressed)
				if not enterPressed then 
					return
				else
					inputScript(mainInput.Text)
				end
			end)
		end
		
		function elements.createSlider(title, min, max, sliderScript)
			local minValue = min or 16
			local maxValue = max or 200
			
			elementOrder += 1
			
			local sliderButton = Instance.new("TextButton")
			local sliderStroke = Instance.new("UIStroke")
			local sliderGradient = Instance.new("UIGradient")
			local sliderCorner = Instance.new("UICorner")
			local sliderTextSizeConstraint = Instance.new("UITextSizeConstraint")

			local sliderBar = Instance.new("TextButton")
			local sliderBarCorner = Instance.new("UICorner")

			local sliderText = Instance.new("TextLabel")
			local sliderTextSizeContraint = Instance.new("UITextSizeConstraint")

			sliderButton.Text = ""
			sliderButton.BackgroundColor3 = Color3.fromRGB(30.00000011175871, 30.00000011175871, 30.00000011175871)
			sliderButton.BackgroundTransparency = 0.5
			sliderButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
			sliderButton.BorderSizePixel = 0
			sliderButton.LayoutOrder = 7
			sliderButton.Position = UDim2.new(0.0365000404, 0, 0.155093119, 0)
			sliderButton.Size = UDim2.new(0.926999986, 0, 0.150000006, 0)
			sliderButton.LayoutOrder = elementOrder
			sliderButton.Name = "Slider"
			sliderButton.Parent = newTab

			sliderStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
			sliderStroke.Color = Color3.fromRGB(122.00000792741776, 97.00000941753387, 223.00001710653305)
			sliderStroke.Thickness = 1.5
			sliderStroke.Parent = sliderButton

			sliderGradient.Color = ColorSequence.new{
				ColorSequenceKeypoint.new(0, Color3.fromRGB(45.00000111758709, 74.0000031888485, 133.00000727176666)),
				ColorSequenceKeypoint.new(1, Color3.fromRGB(162.00000554323196, 81.00000277161598, 206.0000029206276))
			}
			sliderGradient.Rotation = 73
			sliderGradient.Parent = sliderButton

			sliderCorner.CornerRadius = UDim.new(0.100000001, 0)
			sliderCorner.Parent = sliderButton

			sliderTextSizeConstraint.MaxTextSize = 25
			sliderTextSizeConstraint.Parent = sliderButton

			sliderBar.Text = ""
			sliderBar.BackgroundColor3 = Color3.fromRGB(22.000000588595867, 8.000000473111868, 67.00000360608101)
			sliderBar.BackgroundTransparency = 0.5
			sliderBar.BorderColor3 = Color3.fromRGB(0, 0, 0)
			sliderBar.BorderSizePixel = 0
			sliderBar.Position = UDim2.new(0, 0, -7.09687697e-07, 0)
			sliderBar.Size = UDim2.new(0, 0, 1.00000072, 0)
			sliderBar.Name = "SliderBar"
			sliderBar.Parent = sliderButton

			sliderBarCorner.CornerRadius = UDim.new(0.100000001, 0)
			sliderBarCorner.Parent = sliderBar

			sliderText.Font = Enum.Font.GothamBold
			sliderText.Text = string.format("%s %s", minValue, title)
			sliderText.TextColor3 = Color3.fromRGB(255, 255, 255)
			sliderText.TextScaled = true
			sliderText.TextSize = 14
			sliderText.TextWrapped = true
			sliderText.TextXAlignment = Enum.TextXAlignment.Left
			sliderText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			sliderText.BackgroundTransparency = 1
			sliderText.Position = UDim2.new(0.035458345, 0, 0.18378675, 0)
			sliderText.Size = UDim2.new(0.820081115, 0, 0.605058491, 0)
			sliderText.ZIndex = 2
			sliderText.Name = "Title"
			sliderText.Parent = sliderButton

			sliderTextSizeContraint.MaxTextSize = 30
			sliderTextSizeContraint.Parent = sliderText
			
			local mouse = player:GetMouse()
			
			sliderButton.MouseButton1Click:Connect(function()
				local held = false
				local sliderPosX, sliderSizeX

				local function updateSlider(inputX)
					sliderBar.Size = UDim2.new(math.clamp((inputX - sliderPosX) / sliderSizeX, 0, 1), 0, 1, 0)
					local number = math.floor(sliderBar.Size.X.Scale * (maxValue - minValue) + minValue)
					sliderText.Text = string.format("%s %s", number, title)
					
					pcall(function()
						sliderScript(number)
					end)
				end

				local function onInputBegan(input)
					if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
						held = true
						updateSlider(input.Position.X)
					end
				end

				local function onInputEnded(input)
					if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
						held = false
					end
				end

				local function onInputChanged(input)
					if held and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
						updateSlider(input.Position.X)
					end
				end

				sliderBar.MouseButton1Click:Connect(function()
					updateSlider(player:GetMouse().X)
				end)

				sliderButton.InputBegan:Connect(onInputBegan)
				sliderButton.InputEnded:Connect(onInputEnded)

				userInputService.InputChanged:Connect(onInputChanged)

				userInputService.TouchMoved:Connect(function(touch, gameProcessed)
					if held and not gameProcessed then
						updateSlider(touch.Position.X)
					end
				end)

				userInputService.TouchEnded:Connect(onInputEnded)
				
				sliderPosX, sliderSizeX = sliderButton.AbsolutePosition.X, sliderButton.AbsoluteSize.X
			end)
		end
		
		function elements.createToggle(title, toggleScript)
			elementOrder += 1 
			
			local status = false
			
