	local categories = {}
	
	local function createTween(instance, propertyTable, duration, easingStyle)
		local tweenInfo = TweenInfo.new(duration or 0.3, easingStyle or Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
		local tween = tweenService:Create(instance, tweenInfo, propertyTable)
		return tween
	end

	local function setupCategoryToggle(button, scrollingFrame, icon, isActive, imageId, name)
		local function setCategoryActive(button, scrollingFrame, isActive)
			local positionTween = createTween(scrollingFrame, 
				{Position = isActive and UDim2.new(0, 0, 0, 0) or UDim2.new(0, 0, 0.5, 0)}, 
				0.5, Enum.EasingStyle.Sine
			)
			positionTween:Play()

			if isActive then
				local transparencyTween = createTween(button, 
					{BackgroundTransparency = 0.45}, 
					0.3, Enum.EasingStyle.Sine
				)
				local textColorTween = createTween(button.Title, 
					{TextColor3 = icon.ImageColor3}, 
					0.3, Enum.EasingStyle.Sine
				)
				transparencyTween:Play()
				textColorTween:Play()
				Icon_2.Image = imageId
				Title_3.Text = string.format("- %s", name)
			else
				local transparencyTween = createTween(button, 
					{BackgroundTransparency = 1}, 
					0.3, Enum.EasingStyle.Sine
				)
				local textColorTween = createTween(button.Title, 
					{TextColor3 = Color3.fromRGB(163, 163, 163)}, 
					0.3, Enum.EasingStyle.Sine
				)
				transparencyTween:Play()
				textColorTween:Play()
			end
		end

		button.MouseButton1Click:Connect(function()
			for _, category in pairs(categories) do
				if category.button ~= button then
					setCategoryActive(category.button, category.scrollingFrame, false)
					category.scrollingFrame.Visible = false
				end
			end

			scrollingFrame.Visible = true
			setCategoryActive(button, scrollingFrame, true)
		end)

		setCategoryActive(button, scrollingFrame, isActive)
		scrollingFrame.Visible = isActive
	end
	
	local tabOrder = 0
	
	local hub = {}
	
